<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.gone.am.model.AmUser" table="am_user">
<id name="userId" column="user_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="userCd" type="java.lang.String" column="user_cd"/>
<property name="firstName" type="java.lang.String" column="first_name"/>
<property name="lastName" type="java.lang.String" column="last_name"/>
<property name="gender" type="java.lang.String" column="gender"/>
<property name="birthday" type="java.sql.Date" column="birthday"/>
<property name="mobileNumber" type="java.lang.String" column="mobile_number"/>
<property name="email" type="java.lang.String" column="email"/>
<property name="password" type="java.lang.String" column="password"/>
<property name="photoUrl" type="java.lang.String" column="photo_url"/>
<property name="loginCount" type="java.lang.Integer" column="login_count"/>
<property name="description" type="java.lang.String" column="description"/>
<property name="lang" type="java.lang.String" column="lang"/>
<property name="operatorId" type="java.lang.String" column="operator_id"/>
<property name="operatorName" type="java.lang.String" column="operator_name"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="dataStatus" type="java.lang.String" column="data_status"/>
</class>
</hibernate-mapping>
