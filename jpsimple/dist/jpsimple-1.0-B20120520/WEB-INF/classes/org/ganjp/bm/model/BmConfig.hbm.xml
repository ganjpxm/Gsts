<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.bm.model.BmConfig" table="bm_config">
<id name="configId" column="config_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="configKey" type="java.lang.String" column="config_key"/>
<property name="configKeyDisplayName" type="java.lang.String" column="config_key_display_name"/>
<property name="configValue" type="java.lang.String" column="config_value"/>
<property name="description" type="java.lang.String" column="description"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
</class>
</hibernate-mapping>
