<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.bm.model.BmMenu" table="bm_menu">
<id name="menuId" column="menu_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="menuPid" type="java.lang.String" column="menu_pid"/>
<property name="menuCd" type="java.lang.String" column="menu_cd"/>
<property name="menuName" type="java.lang.String" column="menu_name"/>
<property name="menuType" type="java.lang.String" column="menu_type"/>
<property name="url" type="java.lang.String" column="url"/>
<property name="belongUserId" type="java.lang.String" column="belong_user_id"/>
<property name="displayLevel" type="java.lang.Integer" column="display_level"/>
<property name="displayNo" type="java.lang.Integer" column="display_no"/>
<property name="endFlag" type="java.lang.String" column="end_flag"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
</class>
</hibernate-mapping>
