<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.am.model.AmUser" table="am_user">
<id name="userId" column="user_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="userCd" type="java.lang.String" column="user_cd"/>
<property name="userName" type="java.lang.String" column="user_name"/>
<property name="userAlias" type="java.lang.String" column="user_alias"/>
<property name="userLevel" type="java.lang.Integer" column="user_level"/>
<property name="userScore" type="java.lang.Integer" column="user_score"/>
<property name="loginTimes" type="java.lang.Integer" column="login_times"/>
<property name="password" type="java.lang.String" column="password"/>
<property name="gender" type="java.lang.String" column="gender"/>
<property name="qq" type="java.lang.String" column="qq"/>
<property name="email" type="java.lang.String" column="email"/>
<property name="birthYear" type="java.lang.String" column="birth_year"/>
<property name="birthMonth" type="java.lang.String" column="birth_month"/>
<property name="birthDay" type="java.lang.String" column="birth_day"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
</class>
</hibernate-mapping>
