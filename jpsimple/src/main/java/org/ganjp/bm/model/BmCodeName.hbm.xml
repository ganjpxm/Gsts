<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.ganjp.bm.model.BmCodeName" table="bm_code_name">
<id name="codeNameId" column="code_name_id" type="java.lang.String" unsaved-value="">
<generator class="uuid"/>
</id>
<property name="codeNamePid" type="java.lang.String" column="code_name_pid"/>
<property name="code" type="java.lang.String" column="code"/>
<property name="name" type="java.lang.String" column="name"/>
<property name="displayNo" type="java.lang.Integer" column="display_no"/>
<property name="displayLevel" type="java.lang.Integer" column="display_level"/>
<property name="endFlag" type="java.lang.String" column="end_flag"/>
<property name="createDateTime" type="java.sql.Timestamp" column="create_date_time"/>
<property name="modifyTimestamp" type="java.sql.Timestamp" column="modify_timestamp"/>
<property name="dataState" type="java.lang.String" column="data_state"/>
</class>
</hibernate-mapping>
